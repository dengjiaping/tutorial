#import "NNBUserIfShare.h"
static NNBUserIfShare *userInfo;
@implementation NNBUserIfShare
+(NNBUserIfShare *)shareUserInfo
{
    if(!userInfo)
    {
        userInfo = [[NNBUserIfShare alloc] init];
    }
    return userInfo;
}
@end
