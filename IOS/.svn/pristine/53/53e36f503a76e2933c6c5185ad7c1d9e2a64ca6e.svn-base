- (IBAction)getCode:(id)sender {
    [WTItemService nonoBankValidateMobile:self.iPhoneNumber.text header:[[NSString stringWithGetCurrentTime] md5]  successBlock:^(TMRequest *request) {
        
        NNBLoginModel *model = request.result;
        [self jumpUptheData:model.msg];
        
        
        
    } failedBlock:^(TMRequest *request) {
        
        
    }];
}

