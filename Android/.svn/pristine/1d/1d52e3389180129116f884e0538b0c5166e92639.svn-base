<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include  layout="@layout/actionbar_common"/>
    <LinearLayout 
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/ll_content"
        >
      <Spinner
        android:id="@+id/spinner_bank_name"
        android:layout_width="match_parent"
        android:entries="@array/banks"
        android:layout_height="70dp" />
    <Spinner
        android:id="@+id/spinner_province"
        android:layout_width="match_parent"
        android:layout_height="70dp" />
    <Spinner
        android:id="@+id/spinner_city"
        android:layout_width="match_parent"
        android:layout_height="70dp" />

    
    <Button style="@style/OrangeButtonFillet"
        android:id="@+id/bt_complete"
        android:text="完成"/>
    
     <Button style="@style/BlueButtonFillet"
         android:id="@+id/bt_complete_pwd"
        android:text="完成并设置支付密码"/>
     </LinearLayout>


</LinearLayout>
